{
  "name": "land-parcels",
  "version": "1.1.1",
  "description": "Find parcels of land in a grid",
  "main": "dist/main.js",
  "scripts": {
    "test": "nyc mocha ./test/*.ts",
    "start": "tsc && node dist/src/main.js",
    "lint": "eslint src/**/*.ts",
    "build": "tsc"
  },
  "keywords": [
    "land",
    "parcels",
    "grid",
    "neighbours"
  ],
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "recursive": true
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/main.ts"
    ],
    "all": true
  },
  "author": "Pete Edwards",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
